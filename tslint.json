{
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "defaultSeverity": "error",
  "deprecation": {
    "severity": "warn"
  },
  "extends": [
    "tslint:recommended"
  ],
  "forin": true,
  "forof": true,
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "comment-format": false,
    "eofline": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-var-requires": false,
    "variable-name": {
      "options": "allow-leading-underscore"
    }
  },
  "rulesDirectory": []
}