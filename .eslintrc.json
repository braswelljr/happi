{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "eslint",
    "prettier"
  ],
  "extends": [
    "plugin:eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-debugger": "off",
    "no-console": "off",
    "no-multi-spaces": "warn",
    "no-unused-vars": "warn",
    "indent": [
      "warn",
      2
    ]
  }
}
